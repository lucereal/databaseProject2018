
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>Test</title>
	
		<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
    
	</head>
  <body>
		<script type="text/javascript">

			window.onload = function () {
				var chart = new CanvasJS.Chart("chartContainer");
				chart.options.axisY = { title: "# Players" };
				chart.options.title = { text: "Ligue 1 - Players Per Country" };
				chart.options.axisX = {interval: 1};
				var series1 = { //dataSeries - first quarter
					type: "pie",
					name: "Countries",
					showInLegend: true,
					legendText: "{indexLabel}"
				};
	
				chart.options.data = [];
				chart.options.data.push(series1);
				series1.dataPoints = [
						{y: <%= (result[0].numPlayers/sumP)*100 %>, indexLabel: "<%= result[0].Country %>"},
						{y: <%= (result[1].numPlayers/sumP)*100 %>, indexLabel: "<%= result[1].Country %>"},
						{y: <%= (result[2].numPlayers/sumP)*100 %>, indexLabel: "<%= result[2].Country %>"},
						{y: <%= (result[3].numPlayers/sumP)*100 %>, indexLabel: "<%= result[3].Country %>"},
						{y: <%= (result[4].numPlayers/sumP)*100 %>, indexLabel: "<%= result[4].Country %>"},
						{y: <%= (result[5].numPlayers/sumP)*100 %>, indexLabel: "<%= result[5].Country %>"},
						{y: <%= (result[6].numPlayers/sumP)*100 %>, indexLabel: "<%= result[6].Country %>"},
						{y: <%= (result[7].numPlayers/sumP)*100 %>, indexLabel: "<%= result[7].Country %>"},
						{y: <%= (result[8].numPlayers/sumP)*100 %>, indexLabel: "<%= result[8].Country %>"},
						{y: <%= (result[9].numPlayers/sumP)*100 %>, indexLabel: "<%= result[9].Country %>"},
						{y: <%= (otherP/sumP)*100 %>, indexLabel: "Other"}
				];
				chart.render();
			}
		</script>

<div id="chartContainer" style="height: 500px; width: 100%;"></div>
<div class="container">
      <form action="/table4" method="get">
		  <input type="submit" value="Next" > 
       </form>	
		<form action="/table2" method="get">
		  <input type="submit" value="Previous" > 
		</form>		   
</div>
	
	
	</body>

</html>